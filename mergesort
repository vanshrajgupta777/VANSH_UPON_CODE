#include <bits/stdc++.h>
using namespace std;
int merg(int a[],int low,int mid,int high){
    int left=low;
    int right=mid+1;
    vector<int> temp;
    while(left<=mid&&right<=high){
        if (a[left]<a[right]) {
            temp.emplace_back(a[left]);
            left++;
        }
        else{
            temp.emplace_back(a[right]);
            right++;
        }
    }

    while(left<=mid){
        temp.emplace_back(a[left]);
        left++;
    }


    while(right<=high){
        temp.emplace_back(a[right]);
        right++;
    }

    for(int i=low;i<=high;i++){
        a[i]=temp[i-low];
    }
    return 0;
}


void ms(int a[],int low,  int high){
    if(low>=high) return;
    int mid = (high+low)/2;
    ms(a,low,mid);
    ms(a,mid+1,high);
    merg(a,low,mid,high);


}
int main(){
    int a[]={1,2,4,3,6,2,77,22,88,44,99,44};
    int n=sizeof(a)/sizeof(int);
    int low=0,high=n-1;
    ms(a,low,high);
    for(auto x:a){
        cout<<x<<' ';
    }
    return 0;
}
